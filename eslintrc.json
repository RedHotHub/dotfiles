{
  "extends": "airbnb",
  "parser": "babel-eslint",
  "rules": {
    "react/require-extension": "off",
    "newline-per-chained-call": "off",
    "import/no-extraneous-dependencies": [
      "error",
      {
        "devDependencies": [
          "test/**"
        ]
      }
    ]
  },
  "env": {
    "mocha": true,
    "node": true
  }
}
